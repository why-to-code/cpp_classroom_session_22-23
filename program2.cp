// write a program to take input of length and breadth from user and print area
#include<iostream>
using namespace std;

int main(){
    int length , breadth , area;
    // This is called as comment
    cout<<"Enter length and breadth \n";
    cin>>length>>breadth;
    area = length * breadth;
    cout<<"Area is :"<< area <<endl;
    return 0;
}

